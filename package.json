{
  "name": "papermodels",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "prisma generate && vitest run --passWithNoTests",
    "test:watch": "vitest --reporter=verbose",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "format:all": "prettier --write \"src/**/*.ts?(x)\" \"src/*.ts?(x)\""
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-beta.19",
    "@blitzjs/next": "2.0.0-beta.19",
    "@blitzjs/rpc": "2.0.0-beta.19",
    "@cloudinary/react": "1.9.1",
    "@cloudinary/url-gen": "1.9.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/roboto": "4.5.8",
    "@mui/material": "5.11.5",
    "@mui/x-data-grid": "5.17.22",
    "@prisma/client": "4.6.0",
    "blitz": "2.0.0-beta.19",
    "browser-image-resizer": "2.4.1",
    "cloudinary": "1.33.0",
    "firebase": "9.15.0",
    "formik": "2.2.9",
    "next": "12.2.5",
    "playwright-aws-lambda": "^0.5.2",
    "playwright-core": "npm:playwright-chromium@1.1.1",
    "prisma": "4.6.0",
    "puppeteer-serverless": "2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-google-recaptcha-v3": "1.10.1",
    "react-icons": "4.7.1",
    "react-toastify": "9.1.1",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.8",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.9",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/coverage-istanbul": "0.25.8",
    "eslint": "8.27.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.2",
    "jsdom": "20.0.3",
    "lint-staged": "13.0.3",
    "msw": "0.49.2",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "4.4.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.7",
    "ts-jest": "28.0.7",
    "typescript": "^4.8.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "0.28.3"
  },
  "private": true
}
